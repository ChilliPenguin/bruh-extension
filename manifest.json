{
    "name": "Bruh",
    "version": "1.0",
    "version_name":"goodenough",
    "description": "Bruh Moment",
    "manifest_version": 2,
    "browser_action": {
        "default_popup":"popup.html",
        "default_icon": "logo.png"
    },
    "permissions": [
      "storage"
    ],
    "options_ui": {
      "page": "options.html",
    "open_in_tab": false
    },
    "content_scripts": [
      {
        "matches":["<all_urls>"],
        "js":["content.js"]
      }
    ]
}